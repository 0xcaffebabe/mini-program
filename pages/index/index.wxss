.container {
  display: flex;
  height: 100%;
  flex-direction: column;
  align-items: center;
}
page {
  height: 100%;
  background: #b3d4db;
}
.user-avatar {
  width: 200rpx;
  height: 200rpx;
  margin-top: 20rpx;
}
.username {
  font-size: 32rpx;
  font-weight: 600;
}
.moto-container {
  margin-top: 200rpx;
  height: 80rpx;
  width: 200rpx;
  line-height: 80rpx;
  text-align: center;
  border: 1rpx solid #405f80;
  border-radius: 5px;
}
.moto {
  font-size: 22rpx;
}